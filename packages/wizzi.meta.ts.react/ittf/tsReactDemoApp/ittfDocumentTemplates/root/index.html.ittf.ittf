$group
    lang en
    head
        @title ${metaCtx.description}
        meta
            charset UTF-8
        
        link
            rel icon
            type image/svg+xml
            href /vite.svg
        meta
            name viewport
            content width=device-width, initial-scale=1.0
        link
            href index.css
            rel stylesheet        
    body
        div
            id root
        script
            type module
            src /src/main.tsx