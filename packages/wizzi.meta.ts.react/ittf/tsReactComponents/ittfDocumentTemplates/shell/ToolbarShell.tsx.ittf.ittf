$group

    ${'$'}
        var md = {
            styled: true,
            name: "ToolbarShell",
            props: [
                {
                    name: "children",
                    type: "node"
                }
            ]
        }

    v08/ts/react/function${'('}&md)
        < StyledRoot
            + {children}

        ${'$'}append imports
            import 
                @ c
                from '../../themes/ExpoThemeProvider'

        ${'$'}append styles
            styled StyledRoot .div
                display flex
                flex-direction row
                align-items center
                justify-content space-between
                border-bottom
                    => "1px solid " + c('border')
                height 60px
                background-color
                    => c('content')