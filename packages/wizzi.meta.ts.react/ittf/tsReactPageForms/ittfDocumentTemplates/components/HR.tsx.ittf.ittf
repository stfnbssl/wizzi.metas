$group

    ${'$'}
        var md = {
            styled: true,
            name: "HR",
            props: [
            ]
        }
        
    v08/ts/react/function${'('}&md)

        ${'$'}append imports
            import
                @ c
                from '../../themes/ExpoThemeProvider'
        
        ${'$'}append styles
            :interface RootStyleProps
            styled StyledRoot .hr<RootStyleProps>
                background transparent
                border 0
                border-bottom
