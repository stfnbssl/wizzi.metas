$group
    $params name, &jobCtx, ext|@@null
    $if name == 'any_js'
        artifact any_js
            src ./**/*.js.ittf
            schema js
            generator js/module
            extension js
            $if jobCtx.job_model
                model-ref ${jobCtx.job_model}
                    export-name ${jobCtx.job_model}
            $hook
    $elif name == 'any_ts'
        artifact any_ts
            src ./**/*.ts.ittf
            schema ts
            generator ts/module
            extension ts
            $if jobCtx.job_model
                model-ref ${jobCtx.job_model}
                    export-name ${jobCtx.job_model}
            $hook
    $elif name == 'any_tsx'
        artifact any_tsx
            src ./**/*.tsx.ittf
