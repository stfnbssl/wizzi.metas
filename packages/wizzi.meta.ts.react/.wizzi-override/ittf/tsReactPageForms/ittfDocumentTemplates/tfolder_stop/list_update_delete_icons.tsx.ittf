$group
    $params prod

    div
        @ className flex-row
        div
            @ className flex-column m-s
            a_icon( Update, {'/${prod}/update/' + item.id}, warning
                < AddDocumentIcon
                    @ height 16
                    @ width 16
        div
            @ className flex-column m-s
            a_icon( Delete, {'/${prod}/delete/' + item.id}, error
                < AddDocumentIcon
                    @ height 16
                    @ width 16