$group

    $
        var styledWidgetsFolder = "../../widgets/styled";

    ${'$'}
        var md = {
            styled: true,
            name: "EditorShell",
            props: [
            ]
        }

    v08/ts/react/function${'('}&md)
        < StyledRoot
            < StyledLogo
                < AnimatedLogo 

        ${'$'}append imports
            import 
                @ c
                from '../../themes/ExpoThemeProvider'
                
            import 
                @ AnimatedLogo 
                from '${styledWidgetsFolder}/AnimatedLogo'

        ${'$'}append styles
            :interface RootStyleProps
            :interface LogoStyleProps

            styled StyledRoot .div<RootStyleProps>
                display flex
                height 100%
                width 100%
                align-items center
                justify-content center

            styled StyledLogo .div<LogoStyleProps>
                transform scale(0.4)
                opacity 0.2