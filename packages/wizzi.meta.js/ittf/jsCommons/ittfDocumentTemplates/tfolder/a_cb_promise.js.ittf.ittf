$group
	${'$'}params result|notUsed, message|@@null

	=>
		param err
		param ${'$'}{result}
		
        if err
            ${'$'}if message != null
			    error "${'$'}{message}", err
            return
				_ reject
                    ${'$'}if message != null
                        {
                            @ message "${'$'}{message}"
                            @ err
                    ${'$'}else
					    @ err
		${'$'}hook

