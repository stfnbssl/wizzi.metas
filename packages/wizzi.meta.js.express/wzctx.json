{
    "name": "wizzi.meta.js.express",
    "npmName": "@wizzi/meta.js.express",
    "pluginBasename": "",
    "version": "",
    "description": "Wizzi meta plugin for creating wizzi javascript express server packages",
    "author": {
        "name": "Stefano Bassoli",
        "email": "stfn.bssl@gmail.com"
    },
    "github": {
        "userid": "stfnbssl"
    },
    "license": "MIT",
    "metaNamespace": "js.express",
    "Build": {
        "useConfig": true,
        "config": {},
        "isWizziMeta": true,
        "wizziMeta": {
            "plugins": [
                "./wizzi.plugin.css/index.js",
                "./wizzi.plugin.html/index.js",
                "./wizzi.plugin.ittf/index.js",
                "./wizzi.plugin.js/index.js",
                "./wizzi.plugin.json/index.js",
                "./wizzi.plugin.md/index.js",
                "./wizzi.plugin.text/index.js",
                "./wizzi.plugin.wzjob/index.js"
            ],
            "pluginsBaseFolder": "C:/My/wizzi/stfnbssl/wizzi.plugins/packages",
            "globalContext": {
                "wzConfigIsDevelopment": true,
                "wzConfigIsPackageDeploy": false
            }
        },
        "useWizziJsonFactory": true,
        "utils": {
            "useStringifySafe": true
        }
    },
    "Productions": [
        {
            "name": "jsExpress",
            "title": "jsExpress wizzi meta production"
        },
        {
            "name": "jsExpressApp",
            "title": "jsExpressApp wizzi meta production",
            "isPackageMain": true
        },
        {
            "name": "jsExpressBasicApp",
            "title": "jsExpressBasicApp wizzi meta production",
            "isPackageMain": true
        },
        {
            "name": "jsExpressStyles",
            "title": "jsExpressStyles wizzi meta production"
        },
        {
            "name": "jsExpressUtils",
            "title": "jsExpressUtils wizzi meta production"
        },
        {
            "name": "jsExpressWizziSite",
            "title": "jsExpressWizziSite wizzi meta production"
        }
    ]
}
