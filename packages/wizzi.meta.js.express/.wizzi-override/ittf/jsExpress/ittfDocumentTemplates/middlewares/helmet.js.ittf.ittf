$group
    
    const helmet =  require('helmet')

    # https://geshan.com.np/blog/2021/01/nodejs-express-helmet/
    # see https://expressjs.com/it/advanced/best-practice-security.html
    
    set module.exports = {}
    set module.exports.HelmetMiddleware
        => 
            param app
            _ app.use
                _ helmet