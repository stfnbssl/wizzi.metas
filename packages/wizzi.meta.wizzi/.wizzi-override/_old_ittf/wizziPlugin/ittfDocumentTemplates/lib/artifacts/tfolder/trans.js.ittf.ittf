$group
	${'$'}params &md

    $
        var openPar = '(';
        var call = '(&md)';

	${'$'}
        if (!md.rootElement) {
            md.rootElement = md.schema;
        }

    +
	${'$'}hook description
    +
	var util = require('util')
    var async = require('async')
	var verify = require('@wizzi/utils').verify
	var lineParser = verify.lineParser
    var errors = require('../../../../../errors')
	${'$'}hook imports
    +
	var md
        set module.exports =
            { 
    var myname = '${'$'}{md.plugin}.${'$'}{md.schema}.${'$'}{md.name}.trans.main'
    +
	md-trans-main${call}
		${'$'}if md.checkSchema
            if model.wzElement !== '${'$'}{md.rootElement}'
				$$ loog 'wizzi-core', 'transformer', 'model', model
				return
                    _ callback
                        _ error
                            @ 'InvalidArgument'
                            @ 'gen'
                            @ 'Invalid model schema. Expected "${'$'}{md.schema}". Received: ' + model.wzElement
                            @ model
		    +
        try
			${'$'}hook main
		catch ex
			return
				_ callback(ex)
	
    ${'$'}hook

    +
    f_error${openPar} ${'$'}{md.plugin}/lib/artifacts/${'$'}{md.schema}/${'$'}{md.name}/trans/main )
