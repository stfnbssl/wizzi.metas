$group

    ${'$'}include set-ctx-functions
    ${'$'}
        setCtxDevDep ('autoprefixer', '^10.4.19');

    export-default
        {
            { plugins
                { 'tailwindcss/nesting'
                { tailwindcss
                { autoprefixer
    
    $*
    set module.exports
        {
            { plugins
                { tailwindcss
                { autoprefixer
    *$