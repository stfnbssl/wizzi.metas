$group 

    ${'$'}include set-ctx-functions
    ${'$'}
        setCtxDevDep ('prettier', '^3.1.0');
        setCtxScript ('format', 'prettier .  --write');

    bracketSpacing true
    endOfLine "lf"
    jsxBracketSameLine true
    printWidth 120
    semi true
    singleQuote true
    tabWidth 2
    trailingComma "all"  