css
    keyframes spin
        vendor
        keyframe 0%
            transform rotate(0deg)
        keyframe 100%
            transform rotate(360deg)
    . spinner
        border 3px solid #f3f3f3
        border-top 3px solid rgb(235, 137, 82)
        border-radius 50%
        width 25px
        height 25px
        animation spin 1s linear infinite
