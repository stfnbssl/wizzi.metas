module
    kind react
    _ importScripts('https://www.gstatic.com/firebasejs/7.9.1/firebase-app.js')
        #
            # global importScripts, firebase
    _ importScripts('https://www.gstatic.com/firebasejs/7.9.1/firebase-messaging.js')
    _ firebase.initializeApp
        { 
            @ apiKey 'YOUR-API-KEY'
            @ projectId 'YOUR-PROJECT-ID'
            @ messagingSenderId 'YOUR-SENDER-ID'
            @ appId 'YOUR-APP-ID'
    _ firebase.messaging()
