module
    kind react
    export-default 
        function pi
            param n
                :number 
            let v = 0
            for let i = 1; i <= n;
                set v += 1 / i - 1 / (i + 2)
                    # increment by 4
                    # add the value of the series
            return 4 * v
                # apply the factor at last
        # https://stackoverflow.com/a/39575124
