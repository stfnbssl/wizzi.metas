{
    "name": "wizzi.meta.ts.nextjs",
    "npnName": "@wizzi/meta.ts.nextjs",
    "pluginBasename": "",
    "version": "",
    "description": "Wizzi meta plugin for creating wizzi typescript nextjs packages",
    "author": {
        "name": "Stefano Bassoli",
        "email": "stfn.bssl@gmail.com"
    },
    "github": {
        "userid": "stfnbssl"
    },
    "license": "MIT",
    "metaNamespace": "ts.nextjs",
    "Build": {
        "useConfig": true,
        "config": {},
        "isWizziMeta": true,
        "wizziMeta": {
            "plugins": [
                "./wizzi.plugin.css/index.js",
                "./wizzi.plugin.html/index.js",
                "./wizzi.plugin.ittf/index.js",
                "./wizzi.plugin.js/index.js",
                "./wizzi.plugin.json/index.js",
                "./wizzi.plugin.md/index.js",
                "./wizzi.plugin.text/index.js",
                "./wizzi.plugin.wzjob/index.js"
            ],
            "pluginsBaseFolder": "C:/My/wizzi/stfnbssl/wizzi.plugins/packages",
            "globalContext": {
                "wzConfigIsDevelopment": true,
                "wzConfigIsPackageDeploy": false
            }
        },
        "useWizziJsonFactory": true,
        "utils": {
            "useStringifySafe": true
        }
    },
    "Productions": [
        {
            "name": "tsNextjsBasicApp",
            "title": "tsNextjsBasicApp wizzi meta production",
            "isPackageMain": true
        },
        {
            "name": "tsNextjsBlog",
            "title": "tsNextjsBlog wizzi meta production"
        }
    ]
}
