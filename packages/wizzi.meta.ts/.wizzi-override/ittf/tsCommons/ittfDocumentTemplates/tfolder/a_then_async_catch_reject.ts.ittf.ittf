$group
	${'$'}params result|notUsed, typen|@@null, typev|@@null, message|@@null

    ._ then
        async=>
            param ${'$'}{result}
                ${'$'}if typen != null
                    ${'$'}{typen} ${'$'}{typev!=null ? typev : ''}
                ${'$'}else
                    :any
            ${'$'}hook
    catch_reject${'('} ${'$'}{message} )
