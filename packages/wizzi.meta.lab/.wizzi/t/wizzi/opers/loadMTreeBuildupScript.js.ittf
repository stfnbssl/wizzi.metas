$group
    function loadMTreeBuildupScript
        param ittfDocumentUri
        param context
        param plugins
        param callback
        +
        if !callback
            set callback = plugins
            set plugins = null
        +
        _ createWizziFactory
            {
            @ plugins
            a_cb( wf )
                _ wf.loadMTreeBuildupScript
                    @ ittfDocumentUri
                    @ context
                    @ callback
