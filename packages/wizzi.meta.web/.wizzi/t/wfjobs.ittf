        artifact any_js
            src ./**/*.js.ittf
            schema js
            generator js/module
            extension js
            ignore /**/bin/**/*.js
            $if jobCtx.job_model
                model-ref ${jobCtx.job_model}
                    export-name ${jobCtx.job_model}
            $hook
