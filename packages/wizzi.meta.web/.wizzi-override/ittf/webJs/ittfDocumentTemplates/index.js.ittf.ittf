$group
    $if metaCtx.webJs.useJsUtils
        ${'$'}include utils/index
    $if metaCtx.webJs.useJsApi
        ${'$'}include api/index
    $if metaCtx.webJs.useJsControls
        ${'$'}include controls/index
    $if metaCtx.webJs.useJsEditors
        ${'$'}include editors/index
    $if metaCtx.webJs.useJsMvc
        ${'$'}include mvc/index
    $if metaCtx.webJs.useJsDb
        ${'$'}include db/index
    $if metaCtx.webJs.useJsStyles
        ${'$'}include styles/index
