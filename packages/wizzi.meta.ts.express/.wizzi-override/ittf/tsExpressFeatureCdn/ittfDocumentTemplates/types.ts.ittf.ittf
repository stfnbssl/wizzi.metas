$group

    +
    import
        @ Document
        from "mongoose"

    +

    ${'$'}
        var md = {
            Name: 'CdnResource'
        }

    v08/mongo/crud/modeltype${'('}&md)
        ${'$'}append fields
            :p owner
                :string
            :p name
                :string
            :p contents
                :string
            :p created_at
                :ref Date
            :p updated_at
                :ref Date