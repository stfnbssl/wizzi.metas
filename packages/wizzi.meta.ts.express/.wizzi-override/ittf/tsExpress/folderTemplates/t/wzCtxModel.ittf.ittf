$group

    $file __dot__wizzi/models/t/tsExpress.json.ittf.ittf
        ${'$'}{'$'}group

            useExpress true
            { express
                useSslCert ${metaCtx.useSslCert ? "true" : "false"}
                useErrors false
                useStaticFiles true
                { staticFiles
                    basePath "__dirname, '..', '..'"
                    $if metaCtx.tsExpress.useStaticFiles && metaCtx.tsExpress.staticFiles.publicPath
                        publicPath "${metaCtx.tsExpress.staticFiles.publicPath}"
                useSessionStore false
                useExpressValidator true
        
            useSession true
            { session
                name "${'$'}{metaCtx.name}"
                secret "${'$'}{'$'}{stfnbssl_env.sessionSecret}"
        
            noCache true
        
            useSocketIO false 
            { socketIO
            
            useCookie true
            { cookie
        
            useLogger false
            { logger
                useMorgan false
                { morgan
        
            useCompression false
            { compression
                level 6
            
            useHelmet false
            { helmet
            
            usePromiseMiddleware true
            { promiseMiddleware

            $if metaCtx.tsExpress.useCors
                useCors true
                { cors
                    $if metaCtx.tsExpress.cors.clientOrigins && metaCtx.tsExpress.cors.clientOrigins.length > 0
                        [ clientOrigins
                            $foreach item in metaCtx.tsExpress.cors.clientOrigins
                                "${item}"

