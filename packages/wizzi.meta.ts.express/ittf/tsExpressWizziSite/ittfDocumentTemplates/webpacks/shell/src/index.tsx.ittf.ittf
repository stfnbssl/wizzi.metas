$group
    
    ${'$'}if wzCtx.Build.useRegeneratorRuntime
        import regeneratorRuntime from "regenerator-runtime"
    import React from 'react'
    import ReactDOM from 'react-dom'
    $$ import './index.css'
    import App 
        from './components/App'
    +
    _ ReactDOM.render
        < React.StrictMode 
            < App 
                @ previewShown {true}
                @ title {'Hello world packy'}
        _ document.getElementById('root')