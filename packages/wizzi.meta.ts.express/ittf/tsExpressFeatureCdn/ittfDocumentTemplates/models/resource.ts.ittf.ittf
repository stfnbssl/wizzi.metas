$group

    ${'$'}
        var md = {
            Name: 'CdnResource'
        }

    v08/mongo/crud/model${'('}&md)
        ${'$'}append fields
            @ owner String
            @ name String
            @ contents String
            @ created_at Date
            @ updated_at Date

        _ ${'$'}{md.Name}Schema.index
            {
                @ owner 1
                @ name 1
            {
                @ unique true