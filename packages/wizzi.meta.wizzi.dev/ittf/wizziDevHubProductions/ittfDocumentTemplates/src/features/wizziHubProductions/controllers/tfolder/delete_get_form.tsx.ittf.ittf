$group
    ${'$'}params prod
    ${'$'}
        var Prod = _.capitalize(prod);
        var suffix = prod == 'tFolder' || prod == 'job' ? '' : 'Production';
    const id = request.params.id
    ${'$'}$ loog myname + '.getDelete${'$'}{Prod}Form.id', id
    _ get${'$'}{Prod}${'$'}{suffix}ObjectById
        @ id
        ._ then
            =>
                param result
                    :any
                _ renderPageForm
                    @ request
                    @ response
                    {
                        @ type 'success'
                        @ formName 'Delete${'$'}{Prod}${'$'}{suffix}'
                        { formData
                            @ _id result._id
                            @ owner result.owner
                            @ name result.name
                            @ description result.description
                            ${'$'}hook
                    {