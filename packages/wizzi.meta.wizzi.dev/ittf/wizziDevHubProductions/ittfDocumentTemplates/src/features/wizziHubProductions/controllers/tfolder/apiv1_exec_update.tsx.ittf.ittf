$group
    ${'$'}params &ctrl
    ${'$'}
        $$ old var suffix = prod == 'tFolder' || prod == 'job' ? '' : 'Production';
        var suffix = 'Production';

    function exec_update${'$'}{ctrl.Production}${'$'}{suffix}
        param request
            :any
        param response
            :any
        param packiFiles
            :any

        rest/check-params${'('} )
            rest/not-empty${'('} params , id )
            rest/not-empty${'('} body , owner )
            rest/not-empty${'('} body , name )
            rest/not-empty${'('} body , description )
            ${'$'}if ctrl.Production == 'Artifact'
                rest/not-empty${'('} body , mainIttf )
                rest/not-empty${'('} body , wizziSchema )

        _ update${'$'}{ctrl.Production}${'$'}{suffix}
            @ id
            @ owner
            @ name
            @ description
            ${'$'}if ctrl.Production == 'Artifact'
                @ mainIttf
                @ wizziSchema
            @ JSON.stringify(packiFiles)
            ._ then
                =>
                    param result
                        :any
                    ${'$'}$ loog 'put${'$'}{ctrl.Production}${'$'}{suffix}.update.result', result
                    _ invalidateCache
                        @ id
                    _ sendSuccess
                        @ response
                        @ result
            v08/ts/catch_send_fail${'('} exec_update${'$'}{ctrl.Production}${'$'}{suffix}.update${'$'}{ctrl.Production}${'$'}{suffix} )