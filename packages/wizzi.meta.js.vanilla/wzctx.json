{
    "name": "wizzi.meta.js.vanilla",
    "npmName": "@wizzi/meta.js.vanilla",
    "pluginBasename": "",
    "version": "0.0.2",
    "description": "Wizzi meta plugin for creating wizzi javascript vanilla packages",
    "author": {
        "name": "Stefano Bassoli",
        "email": "stfn.bssl@gmail.com"
    },
    "github": {
        "userid": "stfnbssl"
    },
    "license": {
        "type": "MIT",
        "url": "https://github.com/stfnbssl/wizzi.metas/tree/main/packages/wizzi.meta.js.vanilla/license.text",
        "copy": "copy 2024"
    },
    "metaNamespace": "js.vanilla",
    "Build": {
        "useConfig": true,
        "config": {},
        "isWizziMeta": true,
        "wizziMeta": {
            "plugins": [
                "./wizzi.plugin.css/index.js",
                "./wizzi.plugin.html/index.js",
                "./wizzi.plugin.ittf/index.js",
                "./wizzi.plugin.js/index.js",
                "./wizzi.plugin.json/index.js",
                "./wizzi.plugin.md/index.js",
                "./wizzi.plugin.svg/index.js",
                "./wizzi.plugin.text/index.js",
                "./wizzi.plugin.wzjob/index.js"
            ],
            "pluginsBaseFolder": "C:/My/wizzi/stfnbssl/wizzi.plugins/packages",
            "globalContext": {
                "wzConfigIsDevelopment": true,
                "wzConfigIsPackageDeploy": false
            }
        },
        "useWizziJsonFactory": true,
        "utils": {
            "useStringifySafe": true
        },
        "scripts": [
            {
                "name": "x:1",
                "action": "node examples/step_1"
            },
            {
                "name": "x:2",
                "action": "node examples/step_2"
            },
            {
                "name": "x:3",
                "action": "node examples/step_3"
            },
            {
                "name": "x:4",
                "action": "node examples/step_4"
            }
        ]
    },
    "Categories": [
        {
            "name": "javascript"
        },
        {
            "name": "vanillajs"
        }
    ],
    "Productions": [
        {
            "name": "jsVanillaWebpackGetStartedApp",
            "title": "jsVanillaWebpackGetStartedApp wizzi meta production",
            "categories": [
                {
                    "name": "javascript"
                },
                {
                    "name": "vanillajs"
                },
                {
                    "name": "webpack"
                }
            ]
        },
        {
            "name": "jsVanillaUtils",
            "title": "jsVanillaUtils wizzi meta production",
            "categories": [
                {
                    "name": "javascript"
                },
                {
                    "name": "vanillajs"
                },
                {
                    "name": "utilities"
                }
            ]
        }
    ]
}
